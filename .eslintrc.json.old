{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
  	"no-trailing-spaces":"warn",
    "no-unused-vars": "warn",
    "prefer-const": "warn",
	  "indent": ["warn", 4],    
	  "quotes": ["warn", "double"],
    "@typescript-eslint/no-explicit-any": "warn"
  }
}
